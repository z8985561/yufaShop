<!-- 头部搜索区域 -->
<view class='hd-search hd-bg' hidden='{{showSearch}}'>
  <navigator class='icon' url='' hover-class='none'>
    <text class='fa fa-headphones c6'></text>
  </navigator>
  <navigator class='search-box' url="" hover-class='none'>
    <text class='fa fa-search c6'></text>
    <text class='fz-28 c9'> 火锅丸子8.99折</text>
  </navigator>
  <navigator class='icon' url='' hover-class='none'>
    <text class='fa fa-commenting c6'></text>
  </navigator>
</view>
<!-- 头部搜索区域end -->
<view class='mb-80'></view>
<!-- 分类导航 -->
<category-nav class="category-wrap" titleList="{{allProductData}}" bind:tabEvent="tabEventListener"></category-nav>
<!-- 分类导航 -->
<!-- 展示全部类别产品区域 -->
<category-list  style='z-index:-1;' allCateDataList="{{allProductData}}" activeIndex="{{activeIndex}}" subActiveIndex="{{subActiveIndex}}" peopleBuyShow="{{peopleBuyShow}}"></category-list>
<!-- 展示全部类别产品区域 -->

<!-- 底部导航 -->
<view class='footer-nav'>
  <navigator class='nav-item on' url='' hover-class='none'>
    <view>
      <text class='fa fa-home'></text>
    </view>
    <view>首页</view>
  </navigator>
  <navigator class='nav-item' url='/pages/category/category' hover-class='none'>
    <view>
      <text class='fa fa-list-ul'></text>
    </view>
    <view>全部菜品</view>
  </navigator>
  <navigator class='nav-item' url='' hover-class='none'>
    <view>
      <text class='fa fa-file-text-o'></text>
    </view>
    <view>常用清单</view>
  </navigator>
  <navigator class='nav-item' url='' hover-class='none'>
    <view style='position:relative'>
      <text class='fa fa-shopping-cart'></text>
      <text class="weui-badge" style="position: absolute;top: -20rpx;right: 28rpx;">{{cartListNum}}</text>
    </view>
    <view>购物车</view>
  </navigator>
  <navigator class='nav-item' url='' hover-class='none'>
    <view>
      <text class='fa fa-user'></text>
    </view>
    <view>我的</view>
  </navigator>
</view>
<!-- 底部导航end -->
.nav-mask {
    position: fixed;
    z-index: 999;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(0,0,0,0.5);
    display: none;
}

.picker-modal {
    background: #fefefe;
    height: 260px;
    position: fixed;
    bottom: -2rem;
    left: 0;
    right: 0;
    z-index: 1000;
    transform: translate3d(0,100%,0);
}

.picker-modal.city-picker {
    z-index: 2000;
}

.picker-modal.in {
    transition-duration: 300ms;
    transform: translate3d(0,0,0);
    bottom: 0;
}

.picker-modal.out {
    transition-duration: 300ms;
    transform: translate3d(0,100%,0);
}

.picker-modal .picker-control {
    display: flex;
    align-items: center;
    height: 40px;

/* pages/me/order-detail/order-detail.wxss */
page{
  background-color: #f5f5f5;
}
.top{
  padding: 10rpx 30rpx;
  background-color: #f5f5f5;
}
.goods-list{
  padding: 30rpx;
  background-color: #fff;
}
.goods-list navigator{
  margin-bottom: 30rpx;
  border-bottom: 1px solid #f5f5f5;
}
.goods-pic{
  margin-right: 30rpx;
  width: 130rpx;
}
.present{
  margin-top: 4rpx;
  margin-right: 10rpx;
  padding:0 4rpx;
  font-size: 26rpx;
  background-color: #FE893F;
  color: #fff;
}
.list{
  margin-top: 40rpx;
  background-color: #fff;
}
.list > view{
  padding: 20rpx 0;
  border-bottom: 1px solid #f5f5f5;
}
.list > view .left{
  width: 160rpx;
}
.list > view .right{
  width: 75%;
}
.aaa{
  color: #FE893F;
}